/* @configure_input@ */

INSTALL = @INSTALL@;
INSTALL_PROGRAM = [INSTALL];
INSTALL_SCRIPT = [INSTALL];
INSTALL_DATA = [INSTALL] -m 644;

prefix = @prefix@;
/**exec_prefix = @exec_prefix@*/;
/**bindir = @bindir@*/
DMDIR = @DMDIR@;
/**MANDIR = @mandir@*/
PAMDIR = @PAMDIR@;

CFLAGS = @CFLAGS@;
CPPFLAGS = @CPPFLAGS@;
LDFLAGS = @LDFLAGS@;
DEFS = @DEFS@;
LIBS = @LIBS@;
srcdir = @srcdir@;
XCFLAGS = @XCFLAGS@;
XLDFLAGS = @XLDFLAGS@;
XLIBS = @XLIBS@;
GFXCFLAGS = @GFXCFLAGS@;
GFXLDFLAGS = @GFXLDFLAGS@;
GFXLIBS = @GFXLIBS@;
LIBCRYPT = @LIBCRYPT@;
PASSWDLIB = @PASSWDLIB@;
LIBSOCKET = @LIBSCOKET@;
LIBUCB = @LIBUCB@;
LIBUTIL = @LIBUTIL@;
CC = @CC@;
PACKAGE_NAME = @PACKAGE_NAME@;
PACKAGE_VERSION = @PACKAGE_VERSION@;
DESTDIR =;

cc = [CC];
cflags = [CPPFLAGS] [CFLAGS] [DEFS] [prepost -I /include [search_list]];
ldflags = [LDFLAGS];
libs = [LIBS];

wdmLogin_cflags = [cflags] [XCFLAGS] [GFXCFLAGS];
wdmLogin_ldflags = [ldflags] [XLDFLAGS] [GFXLDFLAGS];
wdmLogin_libs = [libs] [XLIBS] [GFXLIBS];

TestLogin_cflags = [cflags];
TestLogin_ldflags = [ldflags];
TestLogin_libs = [libs];

wdm_cflags = [cflags] [XCFLAGS] [GFXCFLAGS];
wdm_ldflags = [ldflags] [XLDFLAGS] [GFXLDFLAGS];
wdm_libs = [libs] [XLIBS] [GFXLIBS] [LIBCRYPT] \
	[PASSWDLIB] [LIBUCB] [LIBUTIL] \
	-lXmu -lXau -lXdmcp;

