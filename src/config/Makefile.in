wdm-config: wdm-config.in Makefile
	-rm -f wdm-config
	sed -e "s:#configdir#:$(configfilesdir):" \
		-e "s:#bindir#:$(BINDIR):" \
		-e "s:#SHUTDOWN#:$(SHUTDOWN):" \
		-e "s:#logdir#:$(LOGDIR):" \
		-e "s:#rundir#:$(RUNDIR):" \
		-e "s:#pixmapfile#:$(PIXMAPFILE):" \
		-e "s:#pixmapcomment#:$(PIXMAPCOMMENT):" \
		-e "s/#wmlist#/$(WMLIST)/" \
		$(srcdir)/wdm-config.in > wdm-config
	chmod 644 wdm-config 

