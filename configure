s%@icewmcm@%$icewmcm%g
s%@XSESSION_PATH@%$XSESSION_PATH%g
s%@xsessioncm@%$xsessioncm%g
s%@STARTKDE_PATH@%$STARTKDE_PATH%g
s%@STARTKDECM@%$STARTKDECM%g
s%@WINMGR_PATH@%$WINMGR_PATH%g
s%@WINMGRCM@%$WINMGRCM%g
s%@WINMGR_NAME@%$WINMGR_NAME%g
